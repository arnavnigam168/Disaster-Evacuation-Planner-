{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "jsx": "react-jsx", 
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowJs": true,   
    "checkJs": false,  
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["src"]
}
